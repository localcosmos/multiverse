                        "1x": "/localcosmos/user_content/content_images/image-3015-250.webp",
                        "2x": "/localcosmos/user_content/content_images/image-3015-500.webp",
                        "4x": "/localcosmos/user_content/content_images/image-3015-1000.webp"
                    }
