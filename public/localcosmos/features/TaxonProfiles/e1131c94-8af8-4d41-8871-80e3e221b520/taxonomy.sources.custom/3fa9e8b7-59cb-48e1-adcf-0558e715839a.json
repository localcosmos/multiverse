        "taxonProfileImages": [
            {
                "text": "Gasblasen können über 5 cm groß sein. Die Seitenzweige sind im Spätwinter keulenförmig.",
                "altText": null,
                "title": "Große Blase und Winterform der Seitenzweige",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-104-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-104-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-104-1000.webp"
                }
            },
            {
                "text": "Die Hauptstiele gabeln sich im Frühling und bilden neue Blasen. Die Seitenzweige schwellen an.",
                "altText": null,
                "title": "Wachsender Knotentang im Frühjahr",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-105-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-105-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-105-1000.webp"
                }
            },
            {
                "text": "Im Sommer bilden die keulenförmigen Seitenstiele Poren zum Entlassen der Eier und Spermien",
                "altText": null,
                "title": "Knotentang mit Vermehrungsblasen",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-106-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-106-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-106-1000.webp"
                }
            },
            {
                "text": "Nach der Paarngszeit fallen die Geschlechtsblasen ab und der Tang wächst kräftig",
                "altText": null,
                "title": "Wachsender Knotentang im Spätsommer",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-107-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-107-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-107-1000.webp"
                }
            },
            {
                "text": "Dank seiner Gasblasen bleibt der Tang der Oberfläche und der Sonne nah",
                "altText": null,
                "title": "Kotentangbestand bei Flut im Felswatt",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-108-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-108-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-108-1000.webp"
                }
            },
            {
                "text": "Plastikmüll wickelt sich oft um Algen und kann zum Abreißen der Alge führen",
                "altText": null,
                "title": "Kontentang mit Ballonschnur",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-109-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-109-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-109-1000.webp"
                }
            },
            {
                "text": "Eine Künstlerin in Nordisland macht Schmuck aus getrockneten Knotentang-Blasen",
                "altText": null,
                "title": "Schmuck aus polierten Knotentang-Blasen",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-110-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-110-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-110-1000.webp"
                }
            },
            {
                "text": "Der Knotentang ist eine Braunalge, kann aber viele Grün- und Gelbtöne aufweisen",
                "altText": null,
                "title": "Knotentang in verschiedenen Farbtönen",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-102-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-102-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-102-1000.webp"
                }
            },
            {
                "text": "Die Gasblasen des Knotentangs sitzen einzeln in der Mitte des ledrigen Stängels",
                "altText": null,
                "title": "Arttypisch sind knotenförmige Blasen im Stiel",
                "imageUrl": {
                    "1x": "/localcosmos/user_content/content_images/image-103-250.webp",
                    "2x": "/localcosmos/user_content/content_images/image-103-500.webp",
                    "4x": "/localcosmos/user_content/content_images/image-103-1000.webp"
                }
            }
        ],
